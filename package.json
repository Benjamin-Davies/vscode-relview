{
  "name": "vscode-relview",
  "displayName": "RelView",
  "description": "RelView language support for VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "relview",
        "aliases": [
          "RelView",
          "relview"
        ],
        "extensions": [
          "prog"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "relview-relation",
        "aliases": [
          "RelView Relation",
          "relview-relation"
        ],
        "extensions": [
          "ascii"
        ],
        "configuration": "./language-configuration.relation.json"
      }
    ],
    "grammars": [
      {
        "language": "relview",
        "scopeName": "source.relview",
        "path": "./syntaxes/relview.tmLanguage.json"
      },
      {
        "language": "relview-relation",
        "scopeName": "source.relview-relation",
        "path": "./syntaxes/relview-relation.tmLanguage.json"
      }
    ]
  }
}
